{
  "name": "@repo/google-sheets",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf -e '.env*'",
    "typecheck": "tsgo --noEmit",
    "build": "bun run build.ts",
    "dev": "bun run build && clasp push --watch",
    "setup": "clasp create --type sheets --title \"OpenSheets\" --rootDir ./dist",
    "clasp:push": "clasp push",
    "clasp:pull": "clasp pull"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.0",
    "@repo/core": "workspace:*",
    "ai": "^6.0.0",
    "gas-client": "^1.2.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@google/clasp": "^3.1.3",
    "@types/bun": "^1.3.5",
    "@types/google-apps-script": "^1.0.100",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20251222.1",
    "bun-plugin-tailwind": "^0.1.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
