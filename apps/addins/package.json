{
  "name": "@repo/addins",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf -e '.env*'",
    "typecheck": "tsgo --noEmit",
    "build": "bun run build.ts",
    "excel:dev": "bun --hot server/index.ts --port 3000",
    "excel:certs": "office-addin-dev-certs install",
    "excel:validate": "office-addin-manifest validate manifest.xml",
    "excel:sideload": "office-addin-debugging start manifest.xml",
    "excel:sideload:stop": "office-addin-debugging stop manifest.xml",
    "sheets:login": "clasp login",
    "sheets:push": "clasp push",
    "sheets:setup": "clasp create --type sheets --title \"OpenSheets\" --rootDir ./dist/google-sheets"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0-beta.91",
    "@ai-sdk/react": "^3.0.0-beta.162",
    "@shikijs/langs": "^3.20.0",
    "@shikijs/themes": "^3.20.0",
    "ai": "^6.0.0-beta.159",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "gas-client": "^1.2.1",
    "hast": "^1.0.0",
    "hast-util-to-jsx-runtime": "^2.3.6",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "radix-ui": "^1.4.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remend": "^1.0.1",
    "shiki": "^3.20.0",
    "tailwind-merge": "^3.4.0",
    "tokenlens": "^1.3.1",
    "unified": "^11.0.5",
    "use-stick-to-bottom": "^1.1.1",
    "vscode-jsonrpc": "^8.2.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@ai-sdk/devtools": "^0.0.1-beta.2",
    "@google/clasp": "^3.1.3",
    "@types/bun": "^1.3.5",
    "@types/google-apps-script": "^2.0.8",
    "@types/hast": "^3.0.4",
    "@types/office-js": "^1.0.565",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20251218.3",
    "bun-plugin-tailwind": "^0.1.2",
    "office-addin-debugging": "^6.0.6",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-manifest": "^2.1.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
