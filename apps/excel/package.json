{
  "name": "@repo/excel",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf -e '.env*'",
    "typecheck": "tsgo --noEmit",
    "build": "bun run build.ts",
    "dev": "PORT=8888 bun --hot dev-server.ts",
    "certs": "office-addin-dev-certs install",
    "validate": "office-addin-manifest validate manifest.dev.xml",
    "sideload": "office-addin-debugging start manifest.dev.xml",
    "sideload:stop": "office-addin-debugging stop manifest.dev.xml"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.0",
    "@repo/core": "workspace:*",
    "ai": "^6.0.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@types/office-js": "^1.0.565",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20251225.1",
    "bun-plugin-tailwind": "^0.1.2",
    "office-addin-debugging": "^6.0.6",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-manifest": "^2.1.2",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
