{
  "name": "opensheets",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "git clean -xdf -e '.env*'",
    "prepare": "git config --local core.hooksPath .githooks",
    "typecheck": "tsgo --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "build": "bun run build.ts",
    "excel:dev": "bun --hot server/index.ts --port 3000",
    "excel:certs": "office-addin-dev-certs install",
    "excel:validate": "office-addin-manifest validate manifest.xml",
    "excel:sideload": "office-addin-debugging start manifest.xml",
    "excel:sideload:stop": "office-addin-debugging stop manifest.xml",
    "sheets:login": "clasp login",
    "sheets:push": "clasp push",
    "sheets:setup": "clasp create --type sheets --title \"OpenSheets\" --rootDir ./dist/google-sheets"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.0-beta.87",
    "@ai-sdk/react": "^3.0.0-beta.159",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "ai": "^6.0.0-beta.156",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "gas-client": "^1.2.1",
    "lucide-react": "^0.560.0",
    "motion": "^12.23.26",
    "nanoid": "^5.1.6",
    "react": "^19",
    "react-dom": "^19",
    "remend": "^1.0.1",
    "shiki": "^3.20.0",
    "tailwind-merge": "^3.4.0",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "vscode-jsonrpc": "^8.2.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@ai-sdk/devtools": "^0.0.1-beta.1",
    "@biomejs/biome": "^2.3.8",
    "@google/clasp": "^3.1.3",
    "@types/bun": "^1.3.5",
    "@types/google-apps-script": "^2.0.8",
    "@types/office-js": "^1.0.561",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript/native-preview": "^7.0.0-dev.20251212.1",
    "bun-plugin-tailwind": "^0.1.2",
    "office-addin-debugging": "^6.0.6",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-manifest": "^2.1.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  }
}
