{
  "name": "opensheets",
  "private": true,
  "packageManager": "bun@1.3.5",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .githooks",
    "clean": "turbo clean",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "turbo typecheck"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/hast": "^3.0.4",
    "turbo": "^2.7.1"
  }
}
